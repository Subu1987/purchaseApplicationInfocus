<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
        id="fiscalYearDialog"
        title="Select Fiscal Year"
        liveChange="_handleFiscalYearSearch"
        confirm="_handleFiscalYearClose"
        cancel="_handleFiscalYearClose"
        multiSelect="true"
        contentWidth="auto"
        class="sapUiPopupWithPadding">
        
        <items>
            <!-- Creating Fiscal Year Options from 2015 to 2030 -->
            <StandardListItem title="2015" type="Active"/>
            <StandardListItem title="2016" type="Active"/>
            <StandardListItem title="2017" type="Active"/>
            <StandardListItem title="2018" type="Active"/>
            <StandardListItem title="2019" type="Active"/>
            <StandardListItem title="2020" type="Active"/>
            <StandardListItem title="2021" type="Active"/>
            <StandardListItem title="2022" type="Active"/>
            <StandardListItem title="2023" type="Active"/>
            <StandardListItem title="2024" type="Active"/>
            <StandardListItem title="2025" type="Active"/>
            <StandardListItem title="2026" type="Active"/>
            <StandardListItem title="2027" type="Active"/>
            <StandardListItem title="2028" type="Active"/>
            <StandardListItem title="2029" type="Active"/>
            <StandardListItem title="2030" type="Active"/>
        </items>

    </SelectDialog>
</core:FragmentDefinition>