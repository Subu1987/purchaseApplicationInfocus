<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns:suite="sap.suite.ui.commons"
	xmlns:viz="sap.viz.ui5.controls" xmlns:vizData="sap.viz.ui5.data" xmlns:vizFeeds="sap.viz.ui5.controls.common.feeds">
	<suite:ChartContainer id="chartContainer" showFullScreen="true" showPersonalization="false" autoAdjustHeight="false"
		title="{globalData>/selectedTabTextSupplierDue}" showLegendButton="false">
		<!-- Chart Content -->
		<suite:content>
			<suite:ChartContainerContent icon="sap-icon://bar-chart" title="Chart View">
				<suite:content>
					<viz:VizFrame id="idVizFrameSupplierDue" vizType="column" height="400px" width="100%" uiConfig="{applicationSet:'fiori'}">
					    <!-- Define dependents for Popover -->
						<viz:dependents>
							<viz:Popover id="idPopOverAllSupplierDue"></viz:Popover>
						</viz:dependents>
						<viz:dataset>
							<vizData:FlattenedDataset data="{top5SupplierDuelistData>/}">
								<vizData:dimensions>
									<vizData:DimensionDefinition name="Supplier Name" value="{top5SupplierDuelistData>name1}"/>
								</vizData:dimensions>
								<vizData:measures>
									<vizData:MeasureDefinition name="Amount" value="{top5SupplierDuelistData>amount}"/>
								</vizData:measures>
							</vizData:FlattenedDataset>
						</viz:dataset>
						<viz:feeds>
							<vizFeeds:FeedItem uid="valueAxis" type="Measure" values="Amount"/>
							<vizFeeds:FeedItem uid="categoryAxis" type="Dimension" values="Supplier Name"/>
							
						</viz:feeds>
						
					</viz:VizFrame>
				</suite:content>
			</suite:ChartContainerContent>
			<!-- Table Content -->
			<suite:ChartContainerContent icon="sap-icon://table-chart" title="Table View">
				<suite:content>
					<table:Table id="idTable" rows="{top5SupplierDuelistData>/}" selectionMode="None">
						<table:columns>
							<table:Column>
								<Label text="Supplier Name"/>
								<table:template>
									<Text text="{top5SupplierDuelistData>name1}"/>
								</table:template>
							</table:Column>
							<!--<table:Column>
								<Label text="Fiscal Year"/>
								<table:template>
									<Text text="{top5SupplierDuelistData>fiscalYear}"/>
								</table:template>
							</table:Column>-->
							<table:Column>
								<Label text="Amount"/>
								<table:template>
									<Text text="{top5SupplierDuelistData>amount}"/>
								</table:template>
							</table:Column>
						</table:columns>
					</table:Table>
				</suite:content>
			</suite:ChartContainerContent>
		</suite:content>
	</suite:ChartContainer>
</core:FragmentDefinition>